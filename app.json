{
  "expo": {
    "name": "Maintainance",
    "slug": "Maintainance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to microphone for voice recording functionality.",
        "NSCameraUsageDescription": "This app needs access to camera to take photos for maintenance records.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select images for maintenance records."
      }
    },
    "android": {
      "package": "com.deepvox.Maintainance",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.334297696005-or01n201vocu6rpnpchvstruijoq7aut",
          "androidClientId": "334297696005-ilhbfcc7v56725om1mpfv8f1t1rf7ki9.apps.googleusercontent.com"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e684609e-3fba-4263-b2cb-0016db1eea1b"
      }
    }
  }
}